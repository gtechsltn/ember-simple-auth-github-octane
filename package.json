  "xo": {
    "envs": [
      "amd",
      "browser"
    ],
    "globals": [],
    "prettier": true,
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "extends": [
      "plugin:ember/recommended"
    ],
    "plugins": [
      "ember"
    ],
    "space": 2,
    "rules": {
      "require-await": 0,
      "no-unused-expressions": 0,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 1
    },
    "overrides": [
      {
        "files": "tests/**/*-test.js",
        "envs": [
          "mocha"
        ],
        "rules": {
          "prefer-arrow-callback": 0
        }
      },
      {
        "files": [
          "ember-cli-build.js",
          "testem.js",
          "blueprints/*/index.js",
          "config/**/*.js",
          "lib/*/index.js"
        ],
        "parserOptions": {
          "sourceType": "script",
          "ecmaVersion": 2015
        },
        "envs": [
          "node"
        ]
      }
    ]
  },
  "prettier": {
    "singleQuote": false,
    "bracketSpacing": true
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "dependencies": {}
